@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>

    <!-- Latest compiled and minified CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet">

    <style type="text/css">
        html, body {
            height: 100%;
        }

        /* CSS only for examples not required for centering */
        .container {
            height: 100%;
        }
    </style>

</head>
<body>


    <div class="container">
        <!--vertical align on parent using my-auto, horizontal align on self mx-auto-->
        <div class="row h-100">
            <div class="col-sm-12 my-auto">
                <div class="mx-auto text-center">
                    <div style="height: 50px">

                        <h1><b>Error |</b> <span style="font-size: 20px;position: relative;bottom: 4px;">An error occurred while processing your request.</span></h1>

                    </div>
                </div>
            </div>
        </div>
    </div>





    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.min.js"></script>
</body>

</html>