    <script type="text/javascript">


        $(document).on('submit', "#LoginForm", function (e) {
            e.preventDefault();

            $.post("/Home/Login", $('#LoginForm').serialize()).done(function (resp) {
                if (resp.isSuccess) {
                    toastr["success"]("Loggedin")
                    window.location.reload();
                }
                else if (!resp.Validate.res) {
                    for (var i = 0; i < resp.Validate.ValidationErrors.length; i++) {
                        toastr["info"](resp.Validate.ValidationErrors[i])
                    }
                } else {
                    toastr["error"](resp.msg)
                }

            }).fail(function (data) {
                console.log(data)
            });
        })

        $(document).on('submit', "#SignupForm", function (e) {

            e.preventDefault();
            if ($('[name= Terms]').prop('checked') == false) {
                toastr["info"]("Please Check Terms & Conditions")

            } else if ($('.Password').val() != $('.CPassword').val()) {
                toastr["info"]("Password & confirm password must be matched")
            }
            else {

                $.post("/Home/Signup", $('#SignupForm').serialize()).done(function (resp) {

                    if (resp.isSuccess) {
                        toastr["info"](resp.msg)
                        $('#SignupForm')[0].reset();
                    }
                    else if (!resp.Validate.res) {
                        for (var i = 0; i < resp.Validate.ValidationErrors.length; i++) {
                            toastr["info"](resp.Validate.ValidationErrors[i])
                        }
                    } else {
                        toastr["error"](resp.msg)
                    }
                }).fail(function (data) {
                    console.log(data)
                });
            }
        })

        $(document).on('click', "#BtnForgotPassword", function (e) {

            $.get("/Home/ForgotPassword", { email: $('#RecoveryEmail').val() }).done(function (resp) {

                if (resp.isSuccess) {
                    toastr["success"](resp.msg)
                }
                else if (!resp.Validate.res) {
                    for (var i = 0; i < resp.Validate.ValidationErrors.length; i++) {
                        toastr["info"](resp.Validate.ValidationErrors[i])
                    }
                } else {
                    toastr["error"](resp.msg)
                }
            }).fail(function (data) {
                toastr["error"](data)
            });

        })


        $(document).on('click', "#LinkForgotPassword", function (e) {
            $('#ForgotPassword').modal('show');
            $('#LoginModal').modal('hide');

        })

        $(document).on('click', "#BtnSignup", function (e) {
            $('#signup').modal('show');
            $.get("/Home/GetStates").done(function (resp) {
                var options = '<option value="">Select</option>';

                for (var i = 0; i < resp.data.length; i++) {
                    options += '<option value="' + resp.data[i].id + '">' + resp.data[i].StateName + '</option>';
                }

                $('.States').html(options);

            }).fail(function (data) {
                toastr["error"](data)
            });


        })


        $(document).on('click', "#BtnChangePassword", function (e) {
            debugger

            var CurrentPassword = $('#CurrentPassword').val()
            var Password = $('#NewPassword').val()
            var ConfirmPassword = $('#ConfirmPassword').val()
            if (Password != ConfirmPassword) {
                toastr["info"]("Password & confirm password must be matched")
            } else {
                $.post("/User/ChangePassword", { CurrentPassword: CurrentPassword,Password: Password, ConfirmPassword: ConfirmPassword }).done(function (resp) {
                    debugger
                    if (resp.isSuccess) {
                        toastr["success"](resp.msg)
                    }
                    else {
                        toastr["error"](resp.msg)
                    }

                }).fail(function (data) {
                    console.log(data)
                });
            }
        })

        // #region Common Js

        $(".toggle-password").click(function () {
            $(this).toggleClass("fa-eye-slash fa-eye");
            var input = $($(this).attr("toggle"));
            if (input.attr("type") == "password") {
                input.attr("type", "text");
            } else {
                input.attr("type", "password");
            }
        });

        toastr.options = {
            "closeButton": true,
            "debug": false,
